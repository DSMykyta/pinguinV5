if (footerEl.dataset.paginationInit === 'true') {
    console.warn('‚ö†Ô∏è Pagination –≤–∂–µ —ñ–Ω—ñ—Ü—ñ–∞–ª—ñ–∑–æ–≤–∞–Ω–æ');
