61 lines
/* css/components/forms/checkbox.css */
/* Standard checkbox styling based on M3 design */

input[type="checkbox"] {
    /* Remove default appearance */
    appearance: none;
    -webkit-appearance: none;
    -moz-appearance: none;
    margin: 0;
    padding: 0;
    border: none;

    /* Size - 24x24px */
    width: 24px;
    height: 24px;
    box-sizing: border-box;
    flex-shrink: 0;

    /* Initial state - rounded square with border */
    background-color: transparent;
    border: 2px solid var(--color-outline);
    border-radius: 4px;

    /* Smooth morphing animation */
    background-size: 18px 18px;
    background-position: center center;
    background-repeat: no-repeat;
    cursor: pointer;

    transition: background-color 0.2s ease, border-radius 0.2s ease, background-image 0.2s ease;
}

/* Hover state - morphs to circle */
input[type="checkbox"]:hover {
    background-color: var(--color-secondary-c);
    border-radius: 24px;
}

/* Checked state - circle with checkmark */
input[type="checkbox"]:checked {
    background-color: var(--color-secondary);
    border-radius: 24px;
    /* Add checkmark SVG */
    background-image: url("data:image/svg+xml,%3csvg xmlns='http://www.w3.org/2000/svg' viewBox='0 0 24 24' width='18' height='18'%3e%3cpath fill='%23FFFFFF' d='M9 16.17L4.83 12l-1.42 1.41L9 19 21 7l-1.41-1.41L9 16.17z'/%3e%3c/svg%3e");
    background-size: 18px 18px;
    background-position: center center;
    background-repeat: no-repeat;
}

/* Focus state for accessibility */
input[type="checkbox"]:focus-visible {
    outline: 2px solid var(--color-main);
    outline-offset: 2px;
}

/* Disabled state */
