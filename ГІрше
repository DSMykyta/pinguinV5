export function showLoader(container, options = {}) {
    const { type = 'spinner', message = '–ó–∞–≤–∞–Ω—Ç–∞–∂–µ–Ω–Ω—è...', overlay = false } = options;
    // Returns API: { element, updateProgress(), updateMessage(), hide() }
}

export function showToast(message, options = {}) {
    const { type = 'info', duration = 3000, position = 'bottom-right' } = options;
    // Shows toast notification
}

export function showErrorDetails(error, context = '') {
    // Shows error toast with context
}

export async function withLoader(container, asyncFn, loaderOptions = {}) {
    // Wraps async operation in loader
}
```

