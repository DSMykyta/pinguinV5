// js/common/ui-loading.js

/**
 * ‚ïî‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïó
 * ‚ïë                    UNIVERSAL LOADING STATES                              ‚ïë
 * ‚ïö‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïù
 *
 * –£–Ω—ñ–≤–µ—Ä—Å–∞–ª—å–Ω—ñ –∫–æ–º–ø–æ–Ω–µ–Ω—Ç–∏ –¥–ª—è –ø–æ–∫–∞–∑—É —Å—Ç–∞–Ω—ñ–≤ –∑–∞–≤–∞–Ω—Ç–∞–∂–µ–Ω–Ω—è:
 * - Spinner (–∫—Ä—É—Ç–∏—Ç—å—Å—è)
 * - Progress dots (–∫—Ä–∞–ø–∫–∏ —â–æ –∞–Ω—ñ–º—É—é—Ç—å—Å—è)
 * - Progress bar (–ø—Ä–æ–≥—Ä–µ—Å-–±–∞—Ä –∑ –≤—ñ–¥—Å–æ—Ç–∫–∞–º–∏)
 */

import { showToast } from './ui-toast.js';

/**
 * –ü–æ–∫–∞–∑–∞—Ç–∏ loader –≤ –∫–æ–Ω—Ç–µ–π–Ω–µ—Ä—ñ
 * @param {string|HTMLElement} container - –°–µ–ª–µ–∫—Ç–æ—Ä –∞–±–æ –µ–ª–µ–º–µ–Ω—Ç –∫–æ–Ω—Ç–µ–π–Ω–µ—Ä–∞
 * @param {Object} options - –ù–∞–ª–∞—à—Ç—É–≤–∞–Ω–Ω—è
 * @param {string} options.type - –¢–∏–ø loader: 'spinner' | 'dots' | 'progress'
 * @param {string} options.message - –ü–æ–≤—ñ–¥–æ–º–ª–µ–Ω–Ω—è
 * @param {boolean} options.overlay - –ü–æ–∫–∞–∑–∞—Ç–∏ –Ω–∞–ø—ñ–≤–ø—Ä–æ–∑–æ—Ä–∏–π overlay
 */
export function showLoader(container, options = {}) {
    const {
        type = 'spinner',
        message = '–ó–∞–≤–∞–Ω—Ç–∞–∂–µ–Ω–Ω—è...',
        overlay = false
    } = options;

    const containerEl = typeof container === 'string'
        ? document.querySelector(container)
        : container;

    if (!containerEl) {
        console.error('‚ùå –ö–æ–Ω—Ç–µ–π–Ω–µ—Ä –¥–ª—è loader –Ω–µ –∑–Ω–∞–π–¥–µ–Ω–æ:', container);
        return null;
    }

    // –í–∏–¥–∞–ª–∏—Ç–∏ –ø–æ–ø–µ—Ä–µ–¥–Ω—ñ–π loader —è–∫—â–æ —î
    const existingLoader = containerEl.querySelector('.loading-overlay, .loading-state');
    if (existingLoader) {
        existingLoader.remove();
    }

    const loaderEl = document.createElement('div');
    loaderEl.className = overlay ? 'loading-overlay' : 'loading-state';

    let loaderHTML = '';

    switch (type) {
        case 'spinner':
            loaderHTML = `
                <div class="spinner"></div>
                <p class="loading-message">${message}</p>
            `;
            break;

        case 'dots':
            loaderHTML = `
                <div class="loading-dots">
                    <span class="dot"></span>
                    <span class="dot"></span>
                    <span class="dot"></span>
                </div>
                <p class="loading-message">${message}</p>
            `;
            break;

        case 'progress':
            loaderHTML = `
                <div class="progress-bar">
                    <div class="progress-fill" data-progress-fill></div>
                </div>
                <p class="loading-message" data-progress-message>${message}</p>
            `;
            break;
    }

    loaderEl.innerHTML = loaderHTML;
    containerEl.appendChild(loaderEl);

    // –ü–æ–≤–µ—Ä–Ω—É—Ç–∏ API –¥–ª—è –æ–Ω–æ–≤–ª–µ–Ω–Ω—è –ø—Ä–æ–≥—Ä–µ—Å—É
    return {
        element: loaderEl,
        updateProgress: (percent, newMessage) => {
            if (type === 'progress') {
                const fill = loaderEl.querySelector('[data-progress-fill]');
                const msg = loaderEl.querySelector('[data-progress-message]');
                if (fill) fill.style.width = `${percent}%`;
                if (msg && newMessage) msg.textContent = newMessage;
            }
        },
        updateMessage: (newMessage) => {
            const msg = loaderEl.querySelector('.loading-message');
            if (msg) msg.textContent = newMessage;
        },
        hide: () => {
            loaderEl.remove();
        }
    };
}

/**
 * –ü—Ä–∏—Ö–æ–≤–∞—Ç–∏ loader
 * @param {string|HTMLElement} container - –°–µ–ª–µ–∫—Ç–æ—Ä –∞–±–æ –µ–ª–µ–º–µ–Ω—Ç –∫–æ–Ω—Ç–µ–π–Ω–µ—Ä–∞
 */
export function hideLoader(container) {
    const containerEl = typeof container === 'string'
        ? document.querySelector(container)
        : container;

    if (!containerEl) return;

    const loader = containerEl.querySelector('.loading-overlay, .loading-state');
    if (loader) {
        loader.remove();
    }
}

/**
 * –ü–æ–∫–∞–∑–∞—Ç–∏ –ø–æ–º–∏–ª–∫—É –∑ –¥–µ—Ç–∞–ª—å–Ω–æ—é —ñ–Ω—Ñ–æ—Ä–º–∞—Ü—ñ—î—é
 * @param {Error} error - –û–±'—î–∫—Ç –ø–æ–º–∏–ª–∫–∏
 * @param {string} context - –ö–æ–Ω—Ç–µ–∫—Å—Ç –ø–æ–º–∏–ª–∫–∏ (—â–æ —Ä–æ–±–∏–ª–∏)
 */
export function showErrorDetails(error, context = '') {
    console.error(`‚ùå –ü–æ–º–∏–ª–∫–∞${context ? ` (${context})` : ''}:`, error);

    const errorMessage = error.message || '–ù–µ–≤—ñ–¥–æ–º–∞ –ø–æ–º–∏–ª–∫–∞';
    const fullMessage = context
        ? `${context}: ${errorMessage}`
        : errorMessage;

    showToast(fullMessage, 'error', 5000);
}

/**
 * –û–±–≥–æ—Ä–Ω—É—Ç–∏ –∞—Å–∏–Ω—Ö—Ä–æ–Ω–Ω—É –æ–ø–µ—Ä–∞—Ü—ñ—é –≤ loader
 * @param {string|HTMLElement} container - –ö–æ–Ω—Ç–µ–π–Ω–µ—Ä –¥–ª—è loader
 * @param {Function} asyncFn - –ê—Å–∏–Ω—Ö—Ä–æ–Ω–Ω–∞ —Ñ—É–Ω–∫—Ü—ñ—è
 * @param {Object} loaderOptions - –ù–∞–ª–∞—à—Ç—É–≤–∞–Ω–Ω—è loader
 */
export async function withLoader(container, asyncFn, loaderOptions = {}) {
    const loader = showLoader(container, loaderOptions);

    try {
        const result = await asyncFn(loader);
        return result;
    } catch (error) {
        showErrorDetails(error, loaderOptions.message || '–û–ø–µ—Ä–∞—Ü—ñ—è');
        throw error;
    } finally {
        if (loader) {
            loader.hide();
        }
    }
}
