 * –ì–æ–ª–æ–≤–Ω–∞ —ñ–Ω—ñ—Ü—ñ–∞–ª—ñ–∑–∞—Ü—ñ—è
 */
export function initBannedWords() {
    console.log('üìã –Ü–Ω—ñ—Ü—ñ–∞–ª—ñ–∑–∞—Ü—ñ—è Banned Words (–Ω–æ–≤–∞ –≤–µ—Ä—Å—ñ—è)...');

    // –ó–∞–≤–∞–Ω—Ç–∞–∂–∏—Ç–∏ UI –æ–¥—Ä–∞–∑—É (–±–µ–∑ –¥–∞–Ω–∏—Ö)
