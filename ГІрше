/**
 * –ì–æ–ª–æ–≤–Ω–∞ —ñ–Ω—ñ—Ü—ñ–∞–ª—ñ–∑–∞—Ü—ñ—è
 */
export function initBannedWords() {
    console.log('üìã –Ü–Ω—ñ—Ü—ñ–∞–ª—ñ–∑–∞—Ü—ñ—è Banned Words (–Ω–æ–≤–∞ –≤–µ—Ä—Å—ñ—è)...');

    // –ó–∞–≤–∞–Ω—Ç–∞–∂–∏—Ç–∏ UI –æ–¥—Ä–∞–∑—É (–±–µ–∑ –¥–∞–Ω–∏—Ö)
    initializeUIWithoutData();

    // –Ü–Ω—ñ—Ü—ñ–∞–ª—ñ–∑—É–≤–∞—Ç–∏ Google Auth
    initGoogleAuth(async () => {
        console.log('‚úÖ Google Auth –≥–æ—Ç–æ–≤–∞');

        // –ó–∞–≤–∞–Ω—Ç–∞–∂–∏—Ç–∏ –¢–Ü–õ–¨–ö–ò –∑–∞–±–æ—Ä–æ–Ω–µ–Ω—ñ —Å–ª–æ–≤–∞
        const { loadBannedWords } = await import('./banned-words-data.js');
        await loadBannedWords();

        // –û—Ç—Ä–∏–º–∞—Ç–∏ —Å–ø–∏—Å–æ–∫ –∞—Ä–∫—É—à—ñ–≤
        const { loadSheetNames } = await import('./banned-words-data.js');
        await loadSheetNames();

        // –û–Ω–æ–≤–∏—Ç–∏ UI –∑ –¥–∞–Ω–∏–º–∏
        await updateUIWithData();

        console.log('‚úÖ Banned Words –≥–æ—Ç–æ–≤–∏–π –¥–æ —Ä–æ–±–æ—Ç–∏');
    });
}

/**
 * –Ü–Ω—ñ—Ü—ñ–∞–ª—ñ–∑–∞—Ü—ñ—è UI –±–µ–∑ –¥–∞–Ω–∏—Ö (–ø–æ–∫–∞–∑–∞—Ç–∏ aside, –ø–æ—Ä–æ–∂–Ω—ñ —Ç–∞–±–ª–∏—Ü—ñ)
 */
async function initializeUIWithoutData() {
    console.log('üé® –Ü–Ω—ñ—Ü—ñ–∞–ª—ñ–∑–∞—Ü—ñ—è UI –±–µ–∑ –¥–∞–Ω–∏—Ö...');

    // 1. –ó–∞–≤–∞–Ω—Ç–∞–∂–∏—Ç–∏ aside
    await loadAside();

    // 2. –Ü–Ω—ñ—Ü—ñ–∞–ª—ñ–∑—É–≤–∞—Ç–∏ –∫–æ–Ω—Ç—Ä–æ–ª—å footer –¥–ª—è —Ç–∞–±—ñ–≤ (ui-tabs.js –≤–∂–µ –∫–µ—Ä—É—î –ø–µ—Ä–µ–º–∏–∫–∞–Ω–Ω—è–º)
    initFooterControl();

    // 3. –ü–æ–∫–∞–∑–∞—Ç–∏ aside-check-panel (–∑ —Å–µ–ª–µ–∫—Ç–∞–º–∏ –¥–ª—è –ø–µ—Ä–µ–≤—ñ—Ä–∫–∏)
    const checkPanel = document.getElementById('aside-check-panel');
    if (checkPanel) checkPanel.style.display = 'block';

    // 4. –ü–æ–∫–∞–∑–∞—Ç–∏ –ø–æ—Ä–æ–∂–Ω—é —Ç–∞–±–ª–∏—Ü—é
    const container = document.getElementById('banned-words-table-container');
    if (container) {
        container.innerHTML = `
            <div class="loading-state">
                <span class="material-symbols-outlined">key</span>
                <p>–ê–≤—Ç–æ—Ä–∏–∑—É–π—Ç–µ—Å—å –¥–ª—è –∑–∞–≤–∞–Ω—Ç–∞–∂–µ–Ω–Ω—è –¥–∞–Ω–∏—Ö</p>
            </div>
        `;
    }

    console.log('‚úÖ UI —ñ–Ω—ñ—Ü—ñ–∞–ª—ñ–∑–æ–≤–∞–Ω–æ –±–µ–∑ –¥–∞–Ω–∏—Ö');
}

/**
 * –û–Ω–æ–≤–∏—Ç–∏ UI –ø—ñ—Å–ª—è –∑–∞–≤–∞–Ω—Ç–∞–∂–µ–Ω–Ω—è –¥–∞–Ω–∏—Ö
 */
async function updateUIWithData() {
    console.log('üé® –û–Ω–æ–≤–ª–µ–Ω–Ω—è UI –∑ –¥–∞–Ω–∏–º–∏...');

    // 1. –ó–∞–≤–∞–Ω—Ç–∞–∂–∏—Ç–∏ aside
    await loadAside();

    // 2. –†–µ–Ω–¥–µ—Ä —Ç–∞–±–ª–∏—Ü—ñ –∑–∞–±–æ—Ä–æ–Ω–µ–Ω–∏—Ö —Å–ª—ñ–≤ –≤ –ø–µ—Ä—à–æ–º—É —Ç–∞–±—ñ
    const { renderBannedWordsManageTab } = await import('./banned-words-manage.js');
    await renderBannedWordsManageTab();

    // 3. –Ü–Ω—ñ—Ü—ñ–∞–ª—ñ–∑—É–≤–∞—Ç–∏ –æ–±—Ä–æ–±–Ω–∏–∫–∏ –ø–æ–¥—ñ–π
    initManageTabEvents();
    initRefreshButton();
    initCheckPanelEvents(); // –ù–æ–≤–∞ —Ñ—É–Ω–∫—Ü—ñ—è –¥–ª—è aside –ø–µ—Ä–µ–≤—ñ—Ä–∫–∏

    // 4. –ü–æ–∫–∞–∑–∞—Ç–∏ controls —ñ aside
    showTabControls('tab-manage');
    showAsidePanels(); // –ü–æ–∫–∞–∑—É—î–º–æ –æ–±–∏–¥–≤—ñ –ø–∞–Ω–µ–ª—ñ

    // 5. –Ü–Ω—ñ—Ü—ñ–∞–ª—ñ–∑—É–≤–∞—Ç–∏ –æ–±—Ä–æ–±–Ω–∏–∫–∏ —Ç–∞–±—ñ–≤
    initTabHandlers();

    console.log('‚úÖ UI —ñ–Ω—ñ—Ü—ñ–∞–ª—ñ–∑–æ–≤–∞–Ω–æ');
}

/**
 * –Ü–Ω—ñ—Ü—ñ–∞–ª—ñ–∑—É–≤–∞—Ç–∏ –æ–±—Ä–æ–±–Ω–∏–∫–∏ –¥–ª—è –≤—Å—ñ—Ö —Ç–∞–±—ñ–≤
 * –ü–û–í–ù–Ü–°–¢–Æ –ü–ï–†–ï–ü–ò–°–ê–ù–û –ó –ù–£–õ–Ø - –ú–ê–ö–°–ò–ú–ê–õ–¨–ù–û –ü–†–û–°–¢–û
 */
/**
 * –ö–æ–Ω—Ç—Ä–æ–ª—å footer - —ñ–Ω—ñ—Ü—ñ–∞–ª—ñ–∑—É—î –û–î–ò–ù –†–ê–ó
 * Footer –¥–∏–≤–∏—Ç—å—Å—è —è–∫–∏–π —Ç–∞–± –∞–∫—Ç–∏–≤–Ω–∏–π —ñ –ø—Ä–∞—Ü—é—î –∑ –π–æ–≥–æ state
 * –ê—Ä—Ö—ñ—Ç–µ–∫—Ç—É—Ä–∞ —è–∫ –≤ pin-map-v1.txt
 */
function initFooterControl() {
    console.log('üë£ –Ü–Ω—ñ—Ü—ñ–∞–ª—ñ–∑–∞—Ü—ñ—è –∫–æ–Ω—Ç—Ä–æ–ª—é footer...');

    const footer = document.querySelector('.fixed-footer');
    if (!footer) {
        console.warn('‚ö†Ô∏è Footer –Ω–µ –∑–Ω–∞–π–¥–µ–Ω–æ');
        return;
    }

    // –ü–æ–∫–∞–∑–∞—Ç–∏ footer
    footer.style.display = 'flex';

    // –î–µ–ª–µ–≥–æ–≤–∞–Ω–∏–π –æ–±—Ä–æ–±–Ω–∏–∫ –∫–ª—ñ–∫—ñ–≤ –ø–æ –ø–∞–≥—ñ–Ω–∞—Ü—ñ—ó
    footer.addEventListener('click', async (e) => {
        const button = e.target.closest('.page-btn, .page-size-option');
        if (!button) return;

        // –ó–Ω–∞–π—Ç–∏ –ê–ö–¢–ò–í–ù–ò–ô —Ç–∞–±
        const activeTab = document.querySelector('.tab-content.is-active');
        if (!activeTab) {
            console.warn('‚ö†Ô∏è –ê–∫—Ç–∏–≤–Ω–∏–π —Ç–∞–± –Ω–µ –∑–Ω–∞–π–¥–µ–Ω–æ');
            return;
        }

        const tabId = activeTab.dataset.tabContent;
        console.log(`üìÑ –ö–ª—ñ–∫ –ø–æ –ø–∞–≥—ñ–Ω–∞—Ü—ñ—ó –≤ —Ç–∞–±—ñ: ${tabId}`);

        // –û—Ç—Ä–∏–º–∞—Ç–∏ state —Ü—å–æ–≥–æ —Ç–∞–±—É
        const tabPagination = bannedWordsState.tabPaginations[tabId];
        if (!tabPagination) {
            console.warn(`‚ö†Ô∏è –ü–∞–≥—ñ–Ω–∞—Ü—ñ—è –¥–ª—è ${tabId} –Ω–µ –∑–Ω–∞–π–¥–µ–Ω–∞`);
            return;
        }

        // –û–±—Ä–æ–±–∏—Ç–∏ –∫–ª—ñ–∫
        const action = button.dataset.action;
        const page = button.dataset.page;
        const newPageSize = button.dataset.pageSize;

        let needsUpdate = false;
        let { currentPage, pageSize, totalItems } = tabPagination;
        const totalPages = Math.ceil(totalItems / pageSize) || 1;

        if (newPageSize) {
            // –ó–º—ñ–Ω–∞ —Ä–æ–∑–º—ñ—Ä—É —Å—Ç–æ—Ä—ñ–Ω–∫–∏
            if (parseInt(newPageSize) !== pageSize) {
                tabPagination.pageSize = parseInt(newPageSize);
                tabPagination.currentPage = 1;
                needsUpdate = true;
            }
        } else if (page) {
            // –ü–µ—Ä–µ—Ö—ñ–¥ –Ω–∞ –∫–æ–Ω–∫—Ä–µ—Ç–Ω—É —Å—Ç–æ—Ä—ñ–Ω–∫—É
            const newPage = parseInt(page);
            if (newPage !== currentPage) {
                tabPagination.currentPage = newPage;
                needsUpdate = true;
            }
        } else if (action) {
            // –°—Ç—Ä—ñ–ª–∫–∏ –≤–ø–µ—Ä–µ–¥/–Ω–∞–∑–∞–¥
            let newPage = currentPage;
            if (action === 'prev') newPage = Math.max(1, currentPage - 1);
            if (action === 'next') newPage = Math.min(totalPages, currentPage + 1);
            if (newPage !== currentPage) {
                tabPagination.currentPage = newPage;
                needsUpdate = true;
            }
        }

        // –í–∏–∫–ª–∏–∫–∞—Ç–∏ onPageChange callback —Ü—å–æ–≥–æ —Ç–∞–±—É
        if (needsUpdate && tabPagination.onPageChange) {
            console.log(`üîÑ –í–∏–∫–ª–∏–∫–∞—é onPageChange –¥–ª—è ${tabId}: —Å—Ç–æ—Ä—ñ–Ω–∫–∞ ${tabPagination.currentPage}, —Ä–æ–∑–º—ñ—Ä ${tabPagination.pageSize}`);
            await tabPagination.onPageChange(tabPagination.currentPage, tabPagination.pageSize);

            // –û–Ω–æ–≤–∏—Ç–∏ UI –ø–∞–≥—ñ–Ω–∞—Ü—ñ—ó
            updatePaginationUI(tabId);
        }
    });

    // –°–ª—É—Ö–∞—Ç–∏ –ø–µ—Ä–µ–º–∏–∫–∞–Ω–Ω—è —Ç–∞–±—ñ–≤ –¥–ª—è –æ–Ω–æ–≤–ª–µ–Ω–Ω—è UI
    document.addEventListener('click', (e) => {
        const tabButton = e.target.closest('[data-tab-target]');
        if (!tabButton) return;

        const tabsContainer = document.getElementById('tabs-head-container');
        if (!tabsContainer || !tabsContainer.contains(tabButton)) return;

        const tabId = tabButton.dataset.tabTarget;
        bannedWordsState.currentTab = tabId;

        // –û–Ω–æ–≤–∏—Ç–∏ UI –ø–∞–≥—ñ–Ω–∞—Ü—ñ—ó –¥–ª—è –Ω–æ–≤–æ–≥–æ —Ç–∞–±—É
        setTimeout(() => updatePaginationUI(tabId), 100);
    });

    console.log('‚úÖ –ö–æ–Ω—Ç—Ä–æ–ª—å footer —ñ–Ω—ñ—Ü—ñ–∞–ª—ñ–∑–æ–≤–∞–Ω–æ');
}

/**
 * –û–Ω–æ–≤–∏—Ç–∏ UI –ø–∞–≥—ñ–Ω–∞—Ü—ñ—ó –¥–ª—è —Ç–∞–±—É
 */
function updatePaginationUI(tabId) {
    const tabPagination = bannedWordsState.tabPaginations[tabId];
    if (!tabPagination) return;

    const { currentPage, pageSize, totalItems } = tabPagination;
    const totalPages = Math.ceil(totalItems / pageSize) || 1;

    // –û–Ω–æ–≤–∏—Ç–∏ label —Ä–æ–∑–º—ñ—Ä—É —Å—Ç–æ—Ä—ñ–Ω–∫–∏
    const pageSizeLabel = document.getElementById('page-size-label');
    if (pageSizeLabel) {
        pageSizeLabel.textContent = pageSize;
    }

    // –û–Ω–æ–≤–∏—Ç–∏ –∫–Ω–æ–ø–∫–∏ –Ω–∞–≤—ñ–≥–∞—Ü—ñ—ó
    const navContainer = document.getElementById('pagination-nav-container');
    if (!navContainer) return;

    const prevDisabled = currentPage === 1 ? 'disabled' : '';
    const nextDisabled = currentPage === totalPages ? 'disabled' : '';

    navContainer.innerHTML = `
        <button class="page-btn" data-action="prev" ${prevDisabled}>
            <span class="material-symbols-outlined">chevron_left</span>
        </button>
        ${generatePageButtons(currentPage, totalPages)}
        <button class="page-btn" data-action="next" ${nextDisabled}>
            <span class="material-symbols-outlined">chevron_right</span>
        </button>
    `;
}

/**
 * –ì–µ–Ω–µ—Ä—É–≤–∞—Ç–∏ –∫–Ω–æ–ø–∫–∏ —Å—Ç–æ—Ä—ñ–Ω–æ–∫
 */
function generatePageButtons(currentPage, totalPages) {
    const buttons = [];
    const maxVisible = 5;

    let startPage = Math.max(1, currentPage - Math.floor(maxVisible / 2));
    let endPage = Math.min(totalPages, startPage + maxVisible - 1);

    if (endPage - startPage < maxVisible - 1) {
        startPage = Math.max(1, endPage - maxVisible + 1);
    }

    for (let i = startPage; i <= endPage; i++) {
        const active = i === currentPage ? 'active' : '';
        buttons.push(`<button class="page-btn ${active}" data-page="${i}">${i}</button>`);
    }

    return buttons.join('');
}

/**
 * –ó–∞–≤–∞–Ω—Ç–∞–∂–∏—Ç–∏ aside –ø–∞–Ω–µ–ª—å
 */
async function loadAside() {
    const panelRightContent = document.getElementById('panel-right-content');
    if (!panelRightContent) return;

    try {
        // –î–æ–¥–∞—î–º–æ timestamp —â–æ–± –æ–±—ñ–π—Ç–∏ –∫–µ—à –±—Ä–∞—É–∑–µ—Ä–∞
        const response = await fetch('templates/aside/aside-banned-words.html');
        if (!response.ok) throw new Error('Failed to load aside');

        const html = await response.text();
        panelRightContent.innerHTML = html;

        // –Ü–Ω—ñ—Ü—ñ–∞–ª—ñ–∑—É–≤–∞—Ç–∏ custom selects –≤ aside
        initCustomSelects(panelRightContent);

        // –Ü–Ω—ñ—Ü—ñ–∞–ª—ñ–∑—É–≤–∞—Ç–∏ dropdown –º–µ–Ω—é
        const { initDropdowns } = await import('../common/ui-dropdown.js');
        initDropdowns();

        // –Ü–Ω—ñ—Ü—ñ–∞–ª—ñ–∑—É–≤–∞—Ç–∏ –ø–µ—Ä–µ–º–∏–∫–∞—á –ø—Ä–∏—Ö–æ–≤—É–≤–∞–Ω–Ω—è –ø–µ—Ä–µ–≤—ñ—Ä–µ–Ω–∏—Ö
        const { initHideCheckedToggle } = await import('./banned-words-check.js');
        initHideCheckedToggle();

        console.log('‚úÖ Aside –∑–∞–≤–∞–Ω—Ç–∞–∂–µ–Ω–æ');
    } catch (error) {
        console.error('‚ùå –ü–æ–º–∏–ª–∫–∞ –∑–∞–≤–∞–Ω—Ç–∞–∂–µ–Ω–Ω—è aside:', error);
    }
}

/**
 * –ü–æ–∫–∞–∑–∞—Ç–∏ –ø–∞–Ω–µ–ª—ñ aside –≤ –∑–∞–ª–µ–∂–Ω–æ—Å—Ç—ñ –≤—ñ–¥ –ø–æ—Ç–æ—á–Ω–æ–≥–æ —Ç–∞–±—É
 */
function showAsidePanels() {
    const currentTab = bannedWordsState.currentTab;

    // –ü–∞–Ω–µ–ª—ñ –≤ aside
    const managePanel = document.getElementById('aside-manage-panel');
    const checkPanel = document.getElementById('aside-check-panel');

    if (currentTab === 'tab-manage') {
        // –ü–æ–∫–∞–∑–∞—Ç–∏ –ø–∞–Ω–µ–ª—å —É–ø—Ä–∞–≤–ª—ñ–Ω–Ω—è
        if (managePanel) managePanel.style.display = 'block';
        if (checkPanel) checkPanel.style.display = 'block';
    } else if (currentTab.startsWith('check-')) {
        // –ü–æ–∫–∞–∑–∞—Ç–∏ –ø–∞–Ω–µ–ª—å –ø–µ—Ä–µ–≤—ñ—Ä–∫–∏
        if (managePanel) managePanel.style.display = 'none';
        if (checkPanel) checkPanel.style.display = 'block';
    }
}

/**
 * –Ü–Ω—ñ—Ü—ñ–∞–ª—ñ–∑–∞—Ü—ñ—è –æ–±—Ä–æ–±–Ω–∏–∫—ñ–≤ –¥–ª—è –ø–∞–Ω–µ–ª—ñ –ø–µ—Ä–µ–≤—ñ—Ä–∫–∏ –≤ aside
 */
function initCheckPanelEvents() {
    const performCheckBtn = document.getElementById('btn-perform-check');

    if (performCheckBtn) {
        performCheckBtn.addEventListener('click', async () => {
            console.log('üîç –ù–∞—Ç–∏—Å–Ω—É—Ç–æ "–í–∏–∫–æ–Ω–∞—Ç–∏ –ø–µ—Ä–µ–≤—ñ—Ä–∫—É"');

            // –û—Ç—Ä–∏–º–∞—Ç–∏ –∑–Ω–∞—á–µ–Ω–Ω—è –∑ —Å–µ–ª–µ–∫—Ç—ñ–≤
            const sheetSelect = document.getElementById('aside-select-sheet');
            const wordSelect = document.getElementById('aside-select-word');
            const columnSelect = document.getElementById('aside-select-column');

            if (!sheetSelect || !wordSelect || !columnSelect) {
                console.error('‚ùå –°–µ–ª–µ–∫—Ç–∏ –Ω–µ –∑–Ω–∞–π–¥–µ–Ω–æ');
                return;
            }

            const selectedSheet = sheetSelect.value;
            const selectedWord = wordSelect.value;
            const selectedColumn = columnSelect.value;

            if (!selectedSheet || !selectedWord || !selectedColumn) {
                alert('–û–±–µ—Ä—ñ—Ç—å –≤—Å—ñ –ø–∞—Ä–∞–º–µ—Ç—Ä–∏ –¥–ª—è –ø–µ—Ä–µ–≤—ñ—Ä–∫–∏');
                return;
            }

            // –ó–±–µ—Ä–µ–≥—Ç–∏ –≤–∏–±—Ä–∞–Ω—ñ –∑–Ω–∞—á–µ–Ω–Ω—è –≤ state
            bannedWordsState.selectedSheet = selectedSheet;
            bannedWordsState.selectedWord = selectedWord;
            bannedWordsState.selectedColumn = selectedColumn;

            // –í–∏–∫–æ–Ω–∞—Ç–∏ –ø–µ—Ä–µ–≤—ñ—Ä–∫—É
            const { performCheck } = await import('./banned-words-check.js');
            const { createCheckResultsTab } = await import('./banned-words-init.js');

            // –°—Ç–≤–æ—Ä–∏—Ç–∏ –∞–±–æ –∞–∫—Ç–∏–≤—É–≤–∞—Ç–∏ —Ç–∞–± –¥–ª—è —Ä–µ–∑—É–ª—å—Ç–∞—Ç—ñ–≤
            await createCheckResultsTab();

            // –í–∏–∫–æ–Ω–∞—Ç–∏ –ø–µ—Ä–µ–≤—ñ—Ä–∫—É
            await performCheck(selectedSheet, selectedWord, selectedColumn);
        });
    }

    // –û–±—Ä–æ–±–Ω–∏–∫ –∑–º—ñ–Ω–∏ –∞—Ä–∫—É—à—É (–∑–∞–≤–∞–Ω—Ç–∞–∂–∏—Ç–∏ –∫–æ–ª–æ–Ω–∫–∏)
    const sheetSelect = document.getElementById('aside-select-sheet');
    if (sheetSelect) {
        sheetSelect.addEventListener('change', async (e) => {
            const sheetName = e.target.value;
            if (sheetName) {
                await loadSheetColumns(sheetName);
            }
        });
    }
}

/**
 * –Ü–Ω—ñ—Ü—ñ–∞–ª—ñ–∑–∞—Ü—ñ—è –æ–±—Ä–æ–±–Ω–∏–∫—ñ–≤ —Ç–∞–±—ñ–≤
 * ui-tabs.js –∞–≤—Ç–æ–º–∞—Ç–∏—á–Ω–æ –æ–±—Ä–æ–±–ª—è—î –ø–µ—Ä–µ–º–∏–∫–∞–Ω–Ω—è
 * –¢—É—Ç —Ç—ñ–ª—å–∫–∏ –¥–æ–¥–∞—î–º–æ –ª–æ–≥—ñ–∫—É –¥–ª—è –ø–æ–∫–∞–∑—É aside –ø–∞–Ω–µ–ª–µ–π
 */
function initTabHandlers() {
    const tabsContainer = document.getElementById('tabs-head-container');
    if (!tabsContainer) return;

    tabsContainer.addEventListener('click', (e) => {
        const tabButton = e.target.closest('[data-tab-target]');
        if (!tabButton) return;

        const tabId = tabButton.dataset.tabTarget;
        bannedWordsState.currentTab = tabId;

        // –ü–æ–∫–∞–∑–∞—Ç–∏ –≤—ñ–¥–ø–æ–≤—ñ–¥–Ω—ñ –ø–∞–Ω–µ–ª—ñ
        showAsidePanels();
        showTabControls(tabId);
    });
}

// –ö–µ—à–æ–≤–∞–Ω—ñ —à–∞–±–ª–æ–Ω–∏
let checkTabTemplate = null;
let checkTabContentTemplate = null;

/**
 * –ó–∞–≤–∞–Ω—Ç–∞–∂–∏—Ç–∏ —à–∞–±–ª–æ–Ω –∫–Ω–æ–ø–∫–∏ —Ç–∞–±—É
 */
async function getCheckTabTemplate() {
    if (checkTabTemplate) return checkTabTemplate;
    try {
        const response = await fetch('/templates/partials/check-tab.html');
        if (!response.ok) throw new Error(`HTTP error! status: ${response.status}`);
        checkTabTemplate = await response.text();
        return checkTabTemplate;
    } catch (e) {
        console.error("–ù–µ –≤–¥–∞–ª–æ—Å—è –∑–∞–≤–∞–Ω—Ç–∞–∂–∏—Ç–∏ —à–∞–±–ª–æ–Ω check-tab.html:", e);
        return '<div class="state-layer"><span class="label">{{selectedSheet}}: {{wordName}}</span></div>';
    }
}

/**
 * –ó–∞–≤–∞–Ω—Ç–∞–∂–∏—Ç–∏ —à–∞–±–ª–æ–Ω –∫–æ–Ω—Ç–µ–Ω—Ç—É —Ç–∞–±—É –ø–µ—Ä–µ–≤—ñ—Ä–∫–∏
 */
async function getCheckTabContentTemplate() {
    if (checkTabContentTemplate) return checkTabContentTemplate;
    try {
        const response = await fetch('/templates/partials/check-tab-content.html');
        if (!response.ok) throw new Error(`HTTP error! status: ${response.status}`);
        checkTabContentTemplate = await response.text();
        return checkTabContentTemplate;
    } catch (e) {
        console.error("–ù–µ –≤–¥–∞–ª–æ—Å—è –∑–∞–≤–∞–Ω—Ç–∞–∂–∏—Ç–∏ —à–∞–±–ª–æ–Ω check-tab-content.html:", e);
        return '';
    }
}

/**
 * –°—Ç–≤–æ—Ä–∏—Ç–∏ –Ω–æ–≤–∏–π —Ç–∞–± –¥–ª—è —Ä–µ–∑—É–ª—å—Ç–∞—Ç—ñ–≤ –ø–µ—Ä–µ–≤—ñ—Ä–∫–∏
 */
async function createCheckResultsTab() {
    const { selectedSheet, selectedWord, selectedColumn } = bannedWordsState;

    // –ó–Ω–∞–π—Ç–∏ —Å–ª–æ–≤–æ –¥–ª—è –Ω–∞–∑–≤–∏ —Ç–∞–±—É
    const word = bannedWordsState.bannedWords.find(w => w.local_id === selectedWord);
    const wordName = word ? (word.name_uk || word.name_ru) : '–°–ª–æ–≤–æ';

    const tabId = `check-${selectedSheet}-${selectedWord}-${selectedColumn}`;

    // –ü–µ—Ä–µ–≤—ñ—Ä–∏—Ç–∏ —á–∏ —Ç–∞–± –≤–∂–µ —ñ—Å–Ω—É—î
    let existingTab = document.querySelector(`[data-tab-target="${tabId}"]`);
    if (existingTab) {
        // –ê–∫—Ç–∏–≤—É–≤–∞—Ç–∏ —ñ—Å–Ω—É—é—á–∏–π —Ç–∞–±
        existingTab.click();
        return;
    }

    // –°—Ç–≤–æ—Ä–∏—Ç–∏ –∫–Ω–æ–ø–∫—É —Ç–∞–±—É
    const tabsContainer = document.getElementById('tabs-head-container');
    const tabButton = document.createElement('button');
    tabButton.className = 'segment tab-button';
    tabButton.dataset.tabTarget = tabId;

    const tabTemplate = await getCheckTabTemplate();
    const tabHtml = tabTemplate
        .replace(/{{selectedSheet}}/g, selectedSheet)
        .replace(/{{wordName}}/g, wordName);
    tabButton.innerHTML = tabHtml;

    tabsContainer.appendChild(tabButton);

    // –°—Ç–≤–æ—Ä–∏—Ç–∏ –∫–æ–Ω—Ç–µ–Ω—Ç —Ç–∞–±—É
    const contentContainer = document.getElementById('sheet-tabs-content-container');
    const tabContent = document.createElement('div');
    tabContent.className = 'tab-content';
    tabContent.dataset.tabContent = tabId;

    const contentTemplate = await getCheckTabContentTemplate();
    const contentHtml = contentTemplate.replace(/{{tabId}}/g, tabId);
    tabContent.innerHTML = contentHtml;

    contentContainer.appendChild(tabContent);

    // –ó–±–µ—Ä–µ–≥—Ç–∏ –ø–∞—Ä–∞–º–µ—Ç—Ä–∏ –ø–µ—Ä–µ–≤—ñ—Ä–∫–∏ –¥–ª—è —Ü—å–æ–≥–æ —Ç–∞–±—É
    tabButton.dataset.checkSheet = selectedSheet;
    tabButton.dataset.checkWord = selectedWord;
    tabButton.dataset.checkColumn = selectedColumn;

    // –î–æ–¥–∞—Ç–∏ –æ–±—Ä–æ–±–Ω–∏–∫ –¥–ª—è –∫–Ω–æ–ø–∫–∏ refresh —Ü—å–æ–≥–æ check —Ç–∞–±—É
    const refreshButton = tabContent.querySelector(`#refresh-check-${tabId}`);
    if (refreshButton) {
        refreshButton.addEventListener('click', async () => {
            console.log(`üîÑ –û–Ω–æ–≤–ª–µ–Ω–Ω—è –¥–∞–Ω–∏—Ö –¥–ª—è —Ç–∞–±—É "${tabId}"`);

            // –û—Ç—Ä–∏–º–∞—Ç–∏ –∑–±–µ—Ä–µ–∂–µ–Ω—ñ –ø–∞—Ä–∞–º–µ—Ç—Ä–∏ –ø–µ—Ä–µ–≤—ñ—Ä–∫–∏ –∑ –∫–Ω–æ–ø–∫–∏ —Ç–∞–±—É
            const sheet = tabButton.dataset.checkSheet;
            const word = tabButton.dataset.checkWord;
            const column = tabButton.dataset.checkColumn;

            // –û–Ω–æ–≤–∏—Ç–∏ state –ø–µ—Ä–µ–¥ –ø–µ—Ä–µ–≤—ñ—Ä–∫–æ—é
            bannedWordsState.selectedSheet = sheet;
            bannedWordsState.selectedWord = word;
            bannedWordsState.selectedColumn = column;

            // –ü–æ–≤—Ç–æ—Ä–Ω–æ –≤–∏–∫–æ–Ω–∞—Ç–∏ –ø–µ—Ä–µ–≤—ñ—Ä–∫—É –¥–ª—è —Ü—å–æ–≥–æ —Ç–∞–±—É
            const { performCheck } = await import('./banned-words-check.js');
            await performCheck(sheet, word, column);
        });
    }

    console.log(`üìã –¢–∞–± —Å—Ç–≤–æ—Ä–µ–Ω–æ: –∫–Ω–æ–ø–∫–∞="${tabId}", –∫–æ–Ω—Ç–µ–Ω—Ç —Å—Ç–≤–æ—Ä–µ–Ω–æ`);
    console.log(`üìã –ö–Ω–æ–ø–∫–∞ –¥–æ–¥–∞–Ω–∞ –¥–æ DOM:`, tabsContainer.contains(tabButton));
    console.log(`üìã –ö–æ–Ω—Ç–µ–Ω—Ç –¥–æ–¥–∞–Ω–æ –¥–æ DOM:`, contentContainer.contains(tabContent));

    // –ê–∫—Ç–∏–≤—É–≤–∞—Ç–∏ –Ω–æ–≤–∏–π —Ç–∞–± —á–µ—Ä–µ–∑ –∫–ª—ñ–∫ (–∑–∞—Ç—Ä–∏–º–∫–∞ –¥–ª—è –æ–Ω–æ–≤–ª–µ–Ω–Ω—è DOM)
    setTimeout(() => {
        console.log(`üñ±Ô∏è –Ü–º—ñ—Ç—É—î–º–æ –∫–ª—ñ–∫ –ø–æ —Ç–∞–±—É "${tabId}"`);
        tabButton.click();
    }, 50);
}

/**
 * –ü–æ–∫–∞–∑–∞—Ç–∏ –≤—ñ–¥–ø–æ–≤—ñ–¥–Ω—ñ controls –¥–ª—è —Ç–∞–±—É
 */
function showTabControls(tabType) {
    const manageControls = document.getElementById('tab-manage-controls');
    const checkControls = document.getElementById('tab-check-controls');

    if (tabType === 'tab-manage') {
        if (manageControls) manageControls.style.display = 'flex';
        if (checkControls) checkControls.style.display = 'none';
    } else {
        if (manageControls) manageControls.style.display = 'none';
        if (checkControls) checkControls.style.display = 'flex';
    }
}

/**
 * –Ü–Ω—ñ—Ü—ñ–∞–ª—ñ–∑–∞—Ü—ñ—è pagination –¥–ª—è tab-manage
 * –°—Ç–≤–æ—Ä—é—î –ø–∞–≥—ñ–Ω–∞—Ü—ñ—é –≤ footer –æ–¥—Ä–∞–∑—É
 */
function initPaginationForBannedWords() {
    const tabId = 'tab-manage';

    // –°—Ç–≤–æ—Ä–∏—Ç–∏ –ø–∞–≥—ñ–Ω–∞—Ü—ñ—é –¥–ª—è tab-manage
    bannedWordsState.tabPaginations[tabId] = {
        currentPage: 1,
        pageSize: 10,
        totalItems: bannedWordsState.bannedWords.length,
        onPageChange: async (page, pageSize) => {
            console.log(`üìÑ Tab-manage: —Å—Ç–æ—Ä—ñ–Ω–∫–∞ ${page}, —Ä–æ–∑–º—ñ—Ä ${pageSize}`);
            bannedWordsState.tabPaginations[tabId].currentPage = page;
            bannedWordsState.tabPaginations[tabId].pageSize = pageSize;
            const { renderBannedWordsTable } = await import('./banned-words-manage.js');
            await renderBannedWordsTable();
        }
    };

    // –û–Ω–æ–≤–∏—Ç–∏ UI –ø–∞–≥—ñ–Ω–∞—Ü—ñ—ó
    updatePaginationUI(tabId);

    console.log(`‚úÖ –ü–∞–≥—ñ–Ω–∞—Ü—ñ—è –¥–ª—è ${tabId} —Å—Ç–≤–æ—Ä–µ–Ω–∞`, bannedWordsState.tabPaginations[tabId]);
}

/**
 * –û–±—Ä–æ–±–Ω–∏–∫–∏ –¥–ª—è —Ç–∞–±—É —É–ø—Ä–∞–≤–ª—ñ–Ω–Ω—è
 */
function initManageTabEvents() {
    // –ö–Ω–æ–ø–∫–∞ –¥–æ–¥–∞–≤–∞–Ω–Ω—è —Å–ª–æ–≤–∞
    const addButton = document.getElementById('btn-add-banned-word');
    if (addButton) {
        addButton.addEventListener('click', async () => {
            const { openBannedWordModal } = await import('./banned-words-manage.js');
            await openBannedWordModal();
        });
    }

    // –ü–æ—à—É–∫
    const searchInput = document.getElementById('search-banned-words');
    const clearSearchBtn = document.getElementById('clear-search-banned-words');

    if (searchInput) {
        searchInput.addEventListener('input', async (e) => {
            bannedWordsState.searchQuery = e.target.value.toLowerCase();

            // –ü–æ–∫–∞–∑–∞—Ç–∏/—Å—Ö–æ–≤–∞—Ç–∏ –∫–Ω–æ–ø–∫—É –æ—á–∏—â–µ–Ω–Ω—è
            if (clearSearchBtn) {
                clearSearchBtn.style.display = e.target.value ? 'flex' : 'none';
            }

            const { renderBannedWordsTable } = await import('./banned-words-manage.js');
            await renderBannedWordsTable();
        });
    }

    // –ö–Ω–æ–ø–∫–∞ –æ—á–∏—â–µ–Ω–Ω—è –ø–æ—à—É–∫—É
    if (clearSearchBtn) {
        clearSearchBtn.addEventListener('click', async () => {
            if (searchInput) {
                searchInput.value = '';
                bannedWordsState.searchQuery = '';
                clearSearchBtn.style.display = 'none';

                const { renderBannedWordsTable } = await import('./banned-words-manage.js');
                await renderBannedWordsTable();
            }
        });
    }

    // –ß–µ–∫–±–æ–∫—Å–∏ –≤–∏–±–æ—Ä—É –∫–æ–ª–æ–Ω–æ–∫ –¥–ª—è –ø–æ—à—É–∫—É
    const searchColumnCheckboxes = document.querySelectorAll('.search-column-checkbox');
    searchColumnCheckboxes.forEach(checkbox => {
        checkbox.addEventListener('change', async () => {
            console.log('üîÑ –ó–º—ñ–Ω–∞ —á–µ–∫–±–æ–∫—Å—É:', checkbox.value, checkbox.checked);

            // –ó—ñ–±—Ä–∞—Ç–∏ –≤—Å—ñ –≤–∏–±—Ä–∞–Ω—ñ –∫–æ–ª–æ–Ω–∫–∏
            const selectedColumns = Array.from(searchColumnCheckboxes)
                .filter(cb => cb.checked)
                .map(cb => cb.value);

            console.log('üìã –í–∏–±—Ä–∞–Ω—ñ –∫–æ–ª–æ–Ω–∫–∏:', selectedColumns);

            bannedWordsState.searchColumns = selectedColumns.length > 0
                ? selectedColumns
                : ['name_uk', 'name_ru']; // Fallback —è–∫—â–æ –Ω—ñ—á–æ–≥–æ –Ω–µ –≤–∏–±—Ä–∞–Ω–æ

            // –ó–ê–í–ñ–î–ò –æ–Ω–æ–≤–ª—é–≤–∞—Ç–∏ —Ä–µ–∑—É–ª—å—Ç–∞—Ç–∏ –ø—Ä–∏ –∑–º—ñ–Ω—ñ —á–µ–∫–±–æ–∫—Å—ñ–≤ (–Ω–∞–≤—ñ—Ç—å —è–∫—â–æ –Ω–µ–º–∞—î –∑–∞–ø–∏—Ç—É)
            const { renderBannedWordsTable } = await import('./banned-words-manage.js');
            await renderBannedWordsTable();

            console.log('‚úÖ –¢–∞–±–ª–∏—Ü—é –æ–Ω–æ–≤–ª–µ–Ω–æ –ø—ñ—Å–ª—è –∑–º—ñ–Ω–∏ —á–µ–∫–±–æ–∫—Å—ñ–≤');
        });
    });

    // –ö–Ω–æ–ø–∫–∞ –æ–Ω–æ–≤–ª–µ–Ω–Ω—è —Ç–∞–±—É
    const refreshTabButton = document.getElementById('refresh-tab-manage');
    if (refreshTabButton) {
        refreshTabButton.addEventListener('click', async () => {
            const { showModal, closeModal } = await import('../common/ui-modal.js');

            await showModal('confirm-refresh-modal');

            // –û–±—Ä–æ–±–Ω–∏–∫ –ø—ñ–¥—Ç–≤–µ—Ä–¥–∂–µ–Ω–Ω—è - –∑ –∑–∞—Ç—Ä–∏–º–∫–æ—é –¥–ª—è DOM
            setTimeout(() => {
                const confirmButton = document.getElementById('confirm-refresh-action');
                console.log('üîç –®—É–∫–∞—é –∫–Ω–æ–ø–∫—É confirm-refresh-action:', confirmButton);

                if (confirmButton) {
                    // –í–∏–¥–∞–ª–∏—Ç–∏ —Å—Ç–∞—Ä—ñ –æ–±—Ä–æ–±–Ω–∏–∫–∏
                    const newButton = confirmButton.cloneNode(true);
                    confirmButton.parentNode.replaceChild(newButton, confirmButton);

                    newButton.addEventListener('click', async () => {
                        console.log('‚úÖ –ö–Ω–æ–ø–∫–∞ "–¢–∞–∫, –æ–Ω–æ–≤–∏—Ç–∏" –Ω–∞—Ç–∏—Å–Ω—É—Ç–∞');
                        closeModal();

                        console.log('üîÑ –û–Ω–æ–≤–ª–µ–Ω–Ω—è —Ç–∞–±—É...');
                        const { loadBannedWords } = await import('./banned-words-data.js');
                        await loadBannedWords();
                        const { renderBannedWordsTable } = await import('./banned-words-manage.js');
                        await renderBannedWordsTable();

                        // –û–Ω–æ–≤–∏—Ç–∏ pagination
                        const tabPagination = bannedWordsState.tabPaginations['tab-manage'];
                        if (tabPagination) {
                            tabPagination.totalItems = bannedWordsState.bannedWords.length;
                            updatePaginationUI('tab-manage');
                        }
                    });
                } else {
                    console.error('‚ùå –ö–Ω–æ–ø–∫—É confirm-refresh-action –Ω–µ –∑–Ω–∞–π–¥–µ–Ω–æ!');
                }
            }, 100);
        });
    }
}

/**
 * –ö–Ω–æ–ø–∫–∞ –æ–Ω–æ–≤–ª–µ–Ω–Ω—è –¥–∞–Ω–∏—Ö
 */
function initRefreshButton() {
    const button = document.getElementById('refresh-data-btn');
    if (!button) return;

    button.addEventListener('click', async () => {
        button.disabled = true;
        console.log('üîÑ –û–Ω–æ–≤–ª–µ–Ω–Ω—è –∑–∞–±–æ—Ä–æ–Ω–µ–Ω–∏—Ö —Å–ª—ñ–≤...');

        try {
            const { loadBannedWords } = await import('./banned-words-data.js');
            await loadBannedWords();

            // –û–Ω–æ–≤–∏—Ç–∏ —Ç–∞–±–ª–∏—Ü—é —è–∫—â–æ –≤ —Ç–∞–±—ñ —É–ø—Ä–∞–≤–ª—ñ–Ω–Ω—è
            if (bannedWordsState.currentTab === 'tab-manage') {
                const { renderBannedWordsTable } = await import('./banned-words-manage.js');
                await renderBannedWordsTable();
            }

            // –û–Ω–æ–≤–∏—Ç–∏ —Å–µ–ª–µ–∫—Ç–∏
            populateCheckSelects();
            updateManageStats();

            console.log('‚úÖ –ó–∞–±–æ—Ä–æ–Ω–µ–Ω—ñ —Å–ª–æ–≤–∞ –æ–Ω–æ–≤–ª–µ–Ω–æ');
        } catch (error) {
            console.error('‚ùå –ü–æ–º–∏–ª–∫–∞ –æ–Ω–æ–≤–ª–µ–Ω–Ω—è:', error);
        } finally {
            button.disabled = false;
        }
    });
}

/**
 * –ó–∞–≤–∞–Ω—Ç–∞–∂–∏—Ç–∏ —Å–ø–∏—Å–æ–∫ –∫–æ–ª–æ–Ω–æ–∫ –¥–ª—è –≤–∏–±—Ä–∞–Ω–æ–≥–æ –∞—Ä–∫—É—à–∞
 */
async function loadSheetColumns(sheetName) {
    const columnSelect = document.getElementById('aside-select-column');
    if (!columnSelect) return;

    try {
        const { getSheetMetadata } = await import('./banned-words-data.js');
        const metadata = await getSheetMetadata(sheetName);

        // –û—á–∏—Å—Ç–∏—Ç–∏ —Å–µ–ª–µ–∫—Ç
        columnSelect.innerHTML = '<option value="">–û–±–µ—Ä—ñ—Ç—å –∫–æ–ª–æ–Ω–∫—É...</option>';

        // –î–æ–¥–∞—Ç–∏ –∫–æ–ª–æ–Ω–∫–∏ —è–∫—ñ –∑–∞–∫—ñ–Ω—á—É—é—Ç—å—Å—è –Ω–∞ Ukr –∞–±–æ Ros
        metadata.headers.forEach((header, index) => {
            if (header.endsWith('Ukr') || header.endsWith('Ros')) {
                const option = document.createElement('option');
                option.value = header;
                option.textContent = header;
                columnSelect.appendChild(option);
            }
        });

        // –†–µ—ñ–Ω—ñ—Ü—ñ–∞–ª—ñ–∑—É–≤–∞—Ç–∏ custom select
        reinitializeCustomSelect(columnSelect);

        console.log(`‚úÖ –ó–∞–≤–∞–Ω—Ç–∞–∂–µ–Ω–æ ${columnSelect.options.length - 1} –∫–æ–ª–æ–Ω–æ–∫ –¥–ª—è –∞—Ä–∫—É—à–∞ "${sheetName}"`);
    } catch (error) {
        console.error('‚ùå –ü–æ–º–∏–ª–∫–∞ –∑–∞–≤–∞–Ω—Ç–∞–∂–µ–Ω–Ω—è –∫–æ–ª–æ–Ω–æ–∫:', error);
    }
}

/**
 * –ó–∞–ø–æ–≤–Ω–∏—Ç–∏ —Å–µ–ª–µ–∫—Ç–∏ –¥–ª—è –ø–µ—Ä–µ–≤—ñ—Ä–∫–∏
 */
function populateCheckSelects() {
    // –ó–∞–ø–æ–≤–Ω–∏—Ç–∏ —Å–µ–ª–µ–∫—Ç –∞—Ä–∫—É—à—ñ–≤
    const sheetSelect = document.getElementById('aside-select-sheet');
    if (sheetSelect && bannedWordsState.sheetNames.length > 0) {
        sheetSelect.innerHTML = '<option value="">–û–±–µ—Ä—ñ—Ç—å –∞—Ä–∫—É—à...</option>' +
            bannedWordsState.sheetNames.map(name =>
                `<option value="${name}">${name}</option>`
            ).join('');
        reinitializeCustomSelect(sheetSelect);
    }

    // –ó–∞–ø–æ–≤–Ω–∏—Ç–∏ —Å–µ–ª–µ–∫—Ç —Å–ª—ñ–≤
    const wordSelect = document.getElementById('aside-select-word');
    if (wordSelect && bannedWordsState.bannedWords.length > 0) {
        wordSelect.innerHTML = '<option value="">–û–±–µ—Ä—ñ—Ç—å —Å–ª–æ–≤–æ...</option>' +
            bannedWordsState.bannedWords.map(word =>
                `<option value="${word.local_id}">${word.name_uk || word.name_ru}</option>`
            ).join('');
        reinitializeCustomSelect(wordSelect);
    }
}

/**
 * –û–Ω–æ–≤–∏—Ç–∏ —Å—Ç–∞—Ç–∏—Å—Ç–∏–∫—É –≤ aside –¥–ª—è —É–ø—Ä–∞–≤–ª—ñ–Ω–Ω—è
 */
function updateManageStats() {
    const totalEl = document.getElementById('banned-words-total');
    if (totalEl) {
        totalEl.textContent = bannedWordsState.bannedWords.length;
    }

    const checkedEl = document.getElementById('banned-words-checked');
    if (checkedEl) {
        const checkedCount = bannedWordsState.bannedWords.filter(w => w.cheaked_line === 'TRUE').length;
        checkedEl.textContent = checkedCount;
    }
}

// –ï–∫—Å–ø–æ—Ä—Ç—É–≤–∞—Ç–∏ —Ñ—É–Ω–∫—Ü—ñ—ó —è–∫—ñ –ø–æ—Ç—Ä—ñ–±–Ω—ñ —ñ–Ω—à–∏–º –º–æ–¥—É–ª—è–º
export { createCheckResultsTab, initPaginationForBannedWords, updateManageStats, populateCheckSelects };
This session is being continued from a previous conversation that ran out of context. The conversation is summarized below:
Analysis:
This is a complex conversation about fixing a pagination system in a banned words checking application. Let me trace through chronologically:

**Initial Problem:**
User reported that tabs switch correctly but there are multiple issues:
1. Two pagination navigations showing (duplicate)
2. Pagination doesn't work correctly - stays on "–ó–∞–±–æ—Ä–æ–Ω–µ–Ω—ñ —Å–ª–æ–≤–∞" tab even when switching to check tabs
3. Yellow toast notification appearing (my new system conflicting with existing)
4. Confusion about `flex: 2` CSS pattern
5. Structure of check tab is fundamentally wrong

**My Initial Approach (WRONG):**
I tried to create a complex system where:
- Footer would be recreated for each tab
- Used `initPagination()` from ui-pagination.js
- Tried to switch footer content dynamically

**User's Feedback:**
User was extremely frustrated, said:
- "–Ω—ñ, —è –æ–æ–±—ñ –∫–∞–∂–µ —â–µ —Ä–∞–∑ –ø—Ä–æ—á–∏—Ç–∞–π pin-map-v1.txt —ñ –Ω–µ —Å—Ç—Ä–∞–ø–æ–Ω—å –º–µ–Ω—ñ –∂–æ–ø—É, —Ç–∏ –Ω—ñ—á–æ–≥–æ –Ω–µ –∑–∏–º—ñ–Ω–∏–≤"
- "–ü–æ—è—Å–Ω–∏ –º–µ–Ω—ñ —á–æ–º—É —Ç–∏ –≤–∂–µ –≥–æ–ª–∏–Ω—É –Ω–µ –º–æ–∂–µ—à –ø—Ä–æ—Å—Ç–æ –≤—ñ–¥—Ç–≤–æ—Ä–∏—Ç–∏ —Ç–∞–± —è–∫–∏–π —ñ—Å–Ω—É—î —â–æ–± —Å—Ç–≤–æ—Ä—é–≤–∞—Ç–∏ —Ç–∞–∫—ñ –∂ —Å–∞–º—ñ?"
- User demanded I read pin-map-v1.txt to understand the correct architecture

**Key Insight from pin-map-v1.txt:**
The correct architecture is:
- Footer is initialized ONCE
- Footer has delegated event handlers that check WHICH TAB IS ACTIVE
- Each tab stores its pagination state in `state[entityType]`
- Footer doesn't switch content, it just calls the right callback based on active tab

**My Final Solution:**
Completely rewrote `initFooterControl()` in banned-words-init.js to:
1. Initialize footer ONCE with delegated click handler
2. Handler finds active tab with `document.querySelector('.tab-content.is-active')`
3. Gets pagination state from `bannedWordsState.tabPaginations[tabId]`
4. Calls that tab's `onPageChange` callback
5. Updates UI with `updatePaginationUI(tabId)` function

Also removed:
- Footer from check-tab-content.html template (it was duplicating)
- My custom toast system (using existing ui-toast.js instead)
- Merged pseudo-table.css and table-fixed-header.css

**Recent Work:**
Just completely rewrote banned-words-init.js with the correct architecture following pin-map-v1.txt pattern.

Summary:
## 1. Primary Request and Intent

The user's primary requests throughout this conversation were:

1. **Fix pagination system**: Pagination was showing duplicate navigations and not working correctly when switching between tabs
2. **Fix tab structure**: Check tabs should be IDENTICAL in structure to the main "–ó–∞–±–æ—Ä–æ–Ω–µ–Ω—ñ —Å–ª–æ–≤–∞" tab
3. **Follow existing architecture**: User demanded I read and follow the architecture in `pin-map-v1.txt` instead of inventing my own system
4. **Remove duplicate systems**: Remove my duplicate toast notification system and use existing one
5. **Simplify CSS**: Merge `pseudo-table.css` and `table-fixed-header.css` into one file
6. **Make loading dynamic**: Add animations to loading states so users can see progress is happening

**Critical User Feedback:**
- "–Ω—ñ, —è –æ–æ–±—ñ –∫–∞–∂–µ —â–µ —Ä–∞–∑ –ø—Ä–æ—á–∏—Ç–∞–π pin-map-v1.txt —ñ –Ω–µ —Å—Ç—Ä–∞–ø–æ–Ω—å –º–µ–Ω—ñ –∂–æ–ø—É, —Ç–∏ –Ω—ñ—á–æ–≥–æ –Ω–µ –∑–∏–º—ñ–Ω–∏–≤" - User frustrated that I didn't actually fix the problem
- "–ü–æ—è—Å–Ω–∏ –º–µ–Ω—ñ —á–æ–º—É —Ç–∏ –≤–∂–µ –≥–æ–ª–∏–Ω—É –Ω–µ –º–æ–∂–µ—à –ø—Ä–æ—Å—Ç–æ –≤—ñ–¥—Ç–≤–æ—Ä–∏—Ç–∏ —Ç–∞–± —è–∫–∏–π —ñ—Å–Ω—É—î —â–æ–± —Å—Ç–≤–æ—Ä—é–≤–∞—Ç–∏ —Ç–∞–∫—ñ –∂ —Å–∞–º—ñ? —Ç–∞–∫ –≤–∞–∂–∫–æ —Å–∫–æ–ø—ñ—é–≤–∞—Ç–∏ –≤—Å–µ –∑ –æ–¥–Ω–æ–≥–æ —ñ –ø–æ—Å—Ç–∞–≤–∏—Ç–∏ –¥–æ –¥—Ä—É–≥–æ–≥–æ?" - User asking why I can't just copy the existing working tab structure

## 2. Key Technical Concepts

- **Event Delegation Pattern**: Footer uses delegated event handlers to respond to clicks
- **Active Tab Detection**: System finds which tab is currently active using `document.querySelector('.tab-content.is-active')`
- **State Management**: Each tab stores its pagination state in `bannedWordsState.tabPaginations[tabId]`
- **Callback Pattern**: Each tab has its own `onPageChange` callback that gets invoked when pagination changes
- **Material Design 3 (MD3)**: UI component system being used
- **Google Sheets API**: Backend data source
- **ES6 Modules**: Dynamic imports with `await import()`
- **CSS Flexbox**: For table column sizing (flex: 1, flex: 2, flex: 3, etc.)
- **Template System**: HTML templates with `{{variable}}` placeholders
- **Progress Bar with Shimmer Animation**: Gradient animation for loading states

## 3. Files and Code Sections

### `js/banned-words/banned-words-init.js` (COMPLETELY REWRITTEN)
**Why Important**: Core initialization file that controls footer pagination system

**Key Changes Made**:
1. Removed complex `switchFooterPagination()` that was recreating pagination
2. Implemented simple delegated event handler pattern from pin-map-v1.txt
3. Added `updatePaginationUI()` function to manually update footer display
4. Added `generatePageButtons()` helper function

**Complete Code for `initFooterControl()` (lines 128-222)**:
```javascript
function initFooterControl() {
    console.log('üë£ –Ü–Ω—ñ—Ü—ñ–∞–ª—ñ–∑–∞—Ü—ñ—è –∫–æ–Ω—Ç—Ä–æ–ª—é footer...');

    const footer = document.querySelector('.fixed-footer');
    if (!footer) {
        console.warn('‚ö†Ô∏è Footer –Ω–µ –∑–Ω–∞–π–¥–µ–Ω–æ');
        return;
    }

    // –ü–æ–∫–∞–∑–∞—Ç–∏ footer
    footer.style.display = 'flex';

    // –î–µ–ª–µ–≥–æ–≤–∞–Ω–∏–π –æ–±—Ä–æ–±–Ω–∏–∫ –∫–ª—ñ–∫—ñ–≤ –ø–æ –ø–∞–≥—ñ–Ω–∞—Ü—ñ—ó
    footer.addEventListener('click', async (e) => {
        const button = e.target.closest('.page-btn, .page-size-option');
        if (!button) return;

        // –ó–Ω–∞–π—Ç–∏ –ê–ö–¢–ò–í–ù–ò–ô —Ç–∞–±
        const activeTab = document.querySelector('.tab-content.is-active');
        if (!activeTab) {
            console.warn('‚ö†Ô∏è –ê–∫—Ç–∏–≤–Ω–∏–π —Ç–∞–± –Ω–µ –∑–Ω–∞–π–¥–µ–Ω–æ');
            return;
        }

        const tabId = activeTab.dataset.tabContent;
        console.log(`üìÑ –ö–ª—ñ–∫ –ø–æ –ø–∞–≥—ñ–Ω–∞—Ü—ñ—ó –≤ —Ç–∞–±—ñ: ${tabId}`);

        // –û—Ç—Ä–∏–º–∞—Ç–∏ state —Ü—å–æ–≥–æ —Ç–∞–±—É
        const tabPagination = bannedWordsState.tabPaginations[tabId];
        if (!tabPagination) {
            console.warn(`‚ö†Ô∏è –ü–∞–≥—ñ–Ω–∞—Ü—ñ—è –¥–ª—è ${tabId} –Ω–µ –∑–Ω–∞–π–¥–µ–Ω–∞`);
            return;
        }

        // –û–±—Ä–æ–±–∏—Ç–∏ –∫–ª—ñ–∫
        const action = button.dataset.action;
        const page = button.dataset.page;
        const newPageSize = button.dataset.pageSize;

        let needsUpdate = false;
        let { currentPage, pageSize, totalItems } = tabPagination;
        const totalPages = Math.ceil(totalItems / pageSize) || 1;

        if (newPageSize) {
            // –ó–º—ñ–Ω–∞ —Ä–æ–∑–º—ñ—Ä—É —Å—Ç–æ—Ä—ñ–Ω–∫–∏
            if (parseInt(newPageSize) !== pageSize) {
                tabPagination.pageSize = parseInt(newPageSize);
                tabPagination.currentPage = 1;
                needsUpdate = true;
            }
        } else if (page) {
            // –ü–µ—Ä–µ—Ö—ñ–¥ –Ω–∞ –∫–æ–Ω–∫—Ä–µ—Ç–Ω—É —Å—Ç–æ—Ä—ñ–Ω–∫—É
            const newPage = parseInt(page);
            if (newPage !== currentPage) {
                tabPagination.currentPage = newPage;
                needsUpdate = true;
            }
        } else if (action) {
            // –°—Ç—Ä—ñ–ª–∫–∏ –≤–ø–µ—Ä–µ–¥/–Ω–∞–∑–∞–¥
            let newPage = currentPage;
            if (action === 'prev') newPage = Math.max(1, currentPage - 1);
            if (action === 'next') newPage = Math.min(totalPages, currentPage + 1);
            if (newPage !== currentPage) {
                tabPagination.currentPage = newPage;
                needsUpdate = true;
            }
        }

        // –í–∏–∫–ª–∏–∫–∞—Ç–∏ onPageChange callback —Ü—å–æ–≥–æ —Ç–∞–±—É
        if (needsUpdate && tabPagination.onPageChange) {
            console.log(`üîÑ –í–∏–∫–ª–∏–∫–∞—é onPageChange –¥–ª—è ${tabId}: —Å—Ç–æ—Ä—ñ–Ω–∫–∞ ${tabPagination.currentPage}, —Ä–æ–∑–º—ñ—Ä ${tabPagination.pageSize}`);
            await tabPagination.onPageChange(tabPagination.currentPage, tabPagination.pageSize);

            // –û–Ω–æ–≤–∏—Ç–∏ UI –ø–∞–≥—ñ–Ω–∞—Ü—ñ—ó
            updatePaginationUI(tabId);
        }
    });

    // –°–ª—É—Ö–∞—Ç–∏ –ø–µ—Ä–µ–º–∏–∫–∞–Ω–Ω—è —Ç–∞–±—ñ–≤ –¥–ª—è –æ–Ω–æ–≤–ª–µ–Ω–Ω—è UI
    document.addEventListener('click', (e) => {
        const tabButton = e.target.closest('[data-tab-target]');
        if (!tabButton) return;

        const tabsContainer = document.getElementById('tabs-head-container');
        if (!tabsContainer || !tabsContainer.contains(tabButton)) return;

        const tabId = tabButton.dataset.tabTarget;
        bannedWordsState.currentTab = tabId;

        // –û–Ω–æ–≤–∏—Ç–∏ UI –ø–∞–≥—ñ–Ω–∞—Ü—ñ—ó –¥–ª—è –Ω–æ–≤–æ–≥–æ —Ç–∞–±—É
        setTimeout(() => updatePaginationUI(tabId), 100);
    });

    console.log('‚úÖ –ö–æ–Ω—Ç—Ä–æ–ª—å footer —ñ–Ω—ñ—Ü—ñ–∞–ª—ñ–∑–æ–≤–∞–Ω–æ');
}
```

**`updatePaginationUI()` function (lines 227-256)**:
```javascript
function updatePaginationUI(tabId) {
    const tabPagination = bannedWordsState.tabPaginations[tabId];
    if (!tabPagination) return;

    const { currentPage, pageSize, totalItems } = tabPagination;
    const totalPages = Math.ceil(totalItems / pageSize) || 1;

    // –û–Ω–æ–≤–∏—Ç–∏ label —Ä–æ–∑–º—ñ—Ä—É —Å—Ç–æ—Ä—ñ–Ω–∫–∏
    const pageSizeLabel = document.getElementById('page-size-label');
    if (pageSizeLabel) {
        pageSizeLabel.textContent = pageSize;
    }

    // –û–Ω–æ–≤–∏—Ç–∏ –∫–Ω–æ–ø–∫–∏ –Ω–∞–≤—ñ–≥–∞—Ü—ñ—ó
    const navContainer = document.getElementById('pagination-nav-container');
    if (!navContainer) return;

    const prevDisabled = currentPage === 1 ? 'disabled' : '';
    const nextDisabled = currentPage === totalPages ? 'disabled' : '';

    navContainer.innerHTML = `
        <button class="page-btn" data-action="prev" ${prevDisabled}>
            <span class="material-symbols-outlined">chevron_left</span>
        </button>
        ${generatePageButtons(currentPage, totalPages)}
        <button class="page-btn" data-action="next" ${nextDisabled}>
            <span class="material-symbols-outlined">chevron_right</span>
        </button>
    `;
}
```

