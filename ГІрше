// –Ü–Ω—ñ—Ü—ñ–∞–ª—ñ–∑–∞—Ü—ñ—è —Å–∏—Å—Ç–µ–º–∏ –ø–µ—Ä–µ–≤—ñ—Ä–∫–∏ –∑–∞–±–æ—Ä–æ–Ω–µ–Ω–∏—Ö —Å–ª—ñ–≤ - –ù–û–í–ê –í–ï–†–°–Ü–Ø

import { initGoogleAuth } from '../auth/google-auth.js';
import { initCustomSelects, reinitializeCustomSelect } from '../common/ui-select.js';
import { initPagination } from '../common/ui-pagination.js';

// –ì–ª–æ–±–∞–ª—å–Ω–∏–π state
export const bannedWordsState = {
    // –î–∞–Ω—ñ
    bannedWords: [],           // –°–ø–∏—Å–æ–∫ –∑–∞–±–æ—Ä–æ–Ω–µ–Ω–∏—Ö —Å–ª—ñ–≤ –∑ —Ç–∞–±–ª–∏—Ü—ñ Banned
    sheetNames: [],            // –ù–∞–∑–≤–∏ –∞—Ä–∫—É—à—ñ–≤ –¥–ª—è –ø–µ—Ä–µ–≤—ñ—Ä–∫–∏

    // –ü–æ—Ç–æ—á–Ω–∏–π —Ç–∞–±
    currentTab: 'tab-manage',  // tab-manage | check-{sheetName}-{wordId}-{column}

